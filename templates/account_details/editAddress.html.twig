{% extends 'account_details/user.html.twig' %}

{% block title %} Edit Profile{% endblock %}

{% block main %}
    {{ form_start(form) }}
    {% for att in form %}
        {% if if not loop.last  %}
            {{ att.vars.label }}
            {{ form_label(att, null, {'label_attr': {'style': 'color:white'}}) }}
            {{ form_widget(att) }}
        {% endif %}
    {% endfor %}
    {{ form_end(form) }}
{% endblock %}
